{
  "builds": [
    {
      "src": "django_api/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "django_api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "django_api.settings"
  }
} 